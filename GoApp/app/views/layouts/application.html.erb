<!DOCTYPE html>
<html>
  <head>
    <title>GoApp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <header> 
      <% if current_user %>
      <%= current_user.username %>
      <% else %>
      <a href= "<%=new_session_url%>"> Log In </a>
      <% end %>
    

    </header>
    <%= yield %>
  </body>
</html>
